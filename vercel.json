{
    "version": 2,
    "functions": {
        "api/index.py": {
            "includeFiles": "llm_agent.py,tool_calls.py,gyg_fetcher.py"
        }
    },
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.py"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}